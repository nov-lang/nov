let std = @import("std")

let fizzBuzz = (n: int) -> string {
    match (n % 3, n % 5) {
        (0, 0) => "FizzBuzz"
        (0, _) => "Fizz"
        (_, 0) => "Buzz"
        _ => "{n}" ; or n.toString()
    }
}

; TODO: use a for loop
let mut i = 0
loop {
    i |> fizzBuzz |> println
    i += 1
    if i == 10 {
        break
    }
}
