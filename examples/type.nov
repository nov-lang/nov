let MyInt = int

@[public]
let MyType = struct {
    x: MyInt = 0
    y: string = "hello"
    z: bool,

    ; `self` is present in all container methods and represents the instance?
    @[public]
    let toString = () -> string {
        "x: {self.x}, y: {self.y}. z: {self.z}"
    }
}

let main = () -> void {
    let item = MyType{ .x = 5, .z = true }
    @println(item) ; @println will call the `toString` method
}
